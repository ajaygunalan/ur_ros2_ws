<?xml version="1.0" encoding="UTF-8"?>
<launch>
  <arg name="debug" default="false" />
  <env
    if="$(arg debug)"
    name="ROSCONSOLE_CONFIG_FILE"
    value="$(find gubbi_icra_2021)/config/debug_rosconsole.conf" />
  <env
    unless="$(arg debug)"
    name="ROSCONSOLE_CONFIG_FILE"
    value="$(find gubbi_icra_2021)/config/info_rosconsole.conf" />
</launch>
